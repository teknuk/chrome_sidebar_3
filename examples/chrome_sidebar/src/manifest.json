{
  "name": "Chrome Sidebar Extension",
  "version": "2.1.0",
  "description": "Example chrome sidebar extension to embed an iframe.",
  "web_accessible_resources": [
    {
      "resources": [
        "entry.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "React Extension"
  },
  "manifest_version": 3,
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "declarativeNetRequest"
  ]
}
